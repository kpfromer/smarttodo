{
  "name": "@smarttodo/frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "yarn generate && gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "cy:open": "cypress open",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.1.1",
    "@rebass/forms": "^4.0.6",
    "@theme-ui/color": "^0.3.1",
    "cross-fetch": "^3.0.5",
    "cypress": "^6.0.1",
    "gatsby": "^2.24.36",
    "gatsby-background-image": "^1.3.1",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-manifest": "^2.4.22",
    "gatsby-plugin-offline": "^3.2.22",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-source-filesystem": "^2.3.14",
    "gatsby-transformer-sharp": "^2.5.7",
    "graphql": "^14.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.2.0",
    "react-icons": "^3.10.0",
    "react-spring": "^8.0.27",
    "react-useportal": "^1.0.13",
    "rebass": "^4.0.7",
    "theme-ui": "^0.3.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@graphql-codegen/cli": "1.17.7",
    "@graphql-codegen/introspection": "1.17.7",
    "@graphql-codegen/typescript": "^1.17.7",
    "@graphql-codegen/typescript-operations": "1.17.7",
    "@graphql-codegen/typescript-react-apollo": "1.17.7",
    "@testing-library/cypress": "^7.0.2",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "@types/jest": "^26.0.9",
    "@types/react-helmet": "^6.1.0",
    "@types/rebass": "^4.0.6",
    "@types/rebass__forms": "^4.0.2",
    "@types/theme-ui": "^0.3.6",
    "babel-jest": "^26.2.2",
    "babel-preset-gatsby": "^0.5.5",
    "cypress": "^6.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.2.2",
    "msw": "^0.20.3",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.([tj]sx?)$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache",
      "public"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ]
  }
}
